Rxvt*background: #252626
Rxvt*foreground: lightgrey
!Rxvt*title:      terminal
Rxvt*saveLines:  1024

!Rxvt*font:     xft:DejaVu Sans Mono:pixelsize=14:antialias=true:hinting=true
!Rxvt*boldFont: xft:DejaVu Sans Mono:bold:pixelsize=14:antialias=true:hinting=true
Rxvt*font:     xft:Ubuntu Mono:pixelsize=16:antialias=true:hinting=true
Rxvt*boldFont: xft:Ubuntu Mono:bold:pixelsize=16:antialias=true:hinting=true
!Rxvt*font:     xft:Inconsolata:pixelsize=16:antialias=true:hinting=true
!Rxvt*boldFont: xft:Inconsolata:bold:pixelsize=16:antialias=true:hinting=true

Rxvt*scrollBar: false

! Enable extensions... see /usr/lib/urxvt/perl
! Enable Ctrl+Alt+c/v... see /usr/lib/uxvt/perl/clipboard
Rxvt.perl-ext-common: clipboard,matcher
! URLs as links (from matcher)
Rxvt.urlLauncher: /usr/bin/luakit
Rxvt.matcher.button: 1
Rxvt.colorUL: #4682B4
! Ctrl+Alt+c/v (from clipboard)
Rxvt.keysym.M-C-c:   perl:clipboard:copy
Rxvt.keysym.M-C-v:   perl:clipboard:paste
!Rxvt.keysym.M-C-v: perl:clipboard:paste_escaped

! Disables keymap mode
Rxvt*iso14755: false

! Transparency
! Rxvt*shading: 0 to 99 darkens, 101 to 200 lightens
!Rxvt*transparent: true
!Rxvt*shading: 30

!!! Zenburn colors
! Normal
! color0    black
! color1    red
! color2    green
! color3    yellow
! color4    blue
! color5    purple
! color6    cyan
! color7    white

Rxvt*color0:    #6c6c6c
Rxvt*color1:    Red
Rxvt*color2:    ForestGreen
Rxvt*color3:    Yellow
Rxvt*color4:    RoyalBlue
Rxvt*color5:    #ea96eb
Rxvt*color6:    #c9caec
Rxvt*color7:    #f2f2f2

! Light
! color8     black
! color9     red
! color10    green
! color11    yellow
! color12    blue
! color13    purple
! color14    cyan
! color15    white

Rxvt*color8:     #747474
Rxvt*color9:     #f99286
Rxvt*color10:    Green
Rxvt*color11:    #fcfbcc
Rxvt*color12:    RoyalBlue
Rxvt*color13:    Purple
Rxvt*color14:    Cyan
Rxvt*color15:    #e2e2e2
!Rxvt*cursorColor: ForestGreen